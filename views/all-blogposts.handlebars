<div class="blogposts">

  {{#each posts }}
  <section class="blogpostCard card p-2 pt-2">
    

    <h2>
      <a href="/post/{{id}}">{{title}}</a>
    </h2>
    
    
    <div>
      Posted by {{User.username}} on {{format_date createdAt}}
    </div>
    <div class="card-body">
      {{body}}
    </div>

  </section>
  {{/each}}
</div>